class Solution {
    public int maxProduct(int[] nums) {
        int maxAns = Integer.MIN_VALUE;
        for(int i = 0;i < nums.length;i++){
            int ans = 1;
            for(int j = i;j < nums.length;j++){
                ans *= nums[j];
                if(ans>maxAns) maxAns = ans;
            }
        }
        return maxAns;
    }
}
