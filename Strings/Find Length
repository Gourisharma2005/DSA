public class LengthWithoutFunction {
    public static void main(String[] args) {
        String s = "hello";
        System.out.println("Length is = " + findLength(s));
    }

    public static int findLength(String str) {
        int count = 0;
        char[] chars = str.toCharArray();

        for (char c : chars) {
            count++;
        }
        return count;
    }
}
